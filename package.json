    "marked-highlight": "^2.1.4",
    "mysql2": "^3.11.5",
    "reflect-metadata": "^0.2.2",
    "simplebar": "^6.2.7",
    "sortablejs": "^1.15.2",
    "svelte": "^4.2.19",
    "svelte-portal": "^2.2.1",
    "svelte-preprocess": "^6.0.3"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "overrides": {
    "strtok3": "8.0.1"
  },
  "resolutions": {
    "strtok3": "8.0.1"
  },
  "pnpm": {

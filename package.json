    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "1.7.26",
    "@types/diffable-html": "^5.0.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.7.5",
    "@types/sortablejs": "^1.15.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "hot-hook": "^0.3.1",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "shiki": "^1.12.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.16",
    "ts-node-maintained": "^10.9.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.3",

@let(variable = 'aaaaaaaaa')

<div x-data="{aaa:'{{ variable}}'}">
    <h1 x-text="aaa"></h1>
</div>

@let(variable2 = '{"a":"aaaaaaaaa"}')

<div x-data="{ aaa: JSON.parse('{{ variable2 }}') }">
    <h2 x-text="aaa.a"></h2>
</div>

<div x-data="{ aaa: {{ js.stringify(variable3) }} }">
    <h3 x-text="aaa.a"></h3>
</div>

<div x-data="{ aaa: {{ js.stringify(currentProject) }} }">
    <h4 x-text="aaa.id"></h4>
</div>
<p>a</p>




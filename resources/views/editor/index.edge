<div x-data="project({{ js.stringify(currentProject) }}, {{ js.stringify(currentProject) }})">
    <span x-text="currentProject.id"></span>
    <br>
    <span x-text="currentProject2.id"></span>
</div>

<script>
    document.addEventListener('alpine:init', function () {
      Alpine.data('project', (currentProject, currentProject2) => ({
        currentProject: currentProject,
        currentProject2: currentProject2,
      }))
    })
  </script>

